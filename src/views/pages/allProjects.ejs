    <main>
        <div class="container">
            <% let default_image="https://stadiumhelp.com/wp-content/uploads/2020/11/ticketmaster-logo.jpeg" ; let
                cols=1; let rows=Math.round(projects.length/cols); %>
                <% let p=0; %>
                    <% for (let r=0; r < cols; r++) { %>
                        <div class="row g-0">

                            <% projects.forEach(function(project) { %>
                                <% let projectName="" ; %>

                                    <div class="col-md-4 g-1">

                                        <div class="card" style="width: 18rem;">
                                            <img class="card-img-top" src="<%=default_image%>" alt="Card image cap">
                                            <div class="card-body">

                                                <h5 class="card-title">
                                                    <%= project.project_name %>
                                                </h5>
                                                <p class="card-subtitle">
                                                    <%= project.description %>
                                                </p>

                                                <a href="/project-<%=projectName%>" class="btn btn-primary">View
                                                    details</a>
                                            </div>
                                        </div>

                                    </div>
                                    <% }); %>
                        </div>
                        <%} %>

        </div>
    </main>

    <%- include ('../partials/footer') %>